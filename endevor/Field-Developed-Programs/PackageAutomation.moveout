             'DA(SYSMD32.NDVR.ADMIN.ENDEVOR.ADM1.CLSTREXX) SHR REUSE'
                  DELIMITED BY SIZE
             INTO ALLOC-TEXT
           END-STRING.
           PERFORM 9000-DYNAMIC-ALLOC-DEALLOC .

