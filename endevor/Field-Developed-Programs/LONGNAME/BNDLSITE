    trace o                                                                     
                                                                                
  "ISREDIT RESET"                                                               
  Message = "****** Tailor the contents of the member "                         
  "ISREDIT LINE_BEFORE 1 = NOTELINE '"Message"'"                                
  Message = "******        to contain Site-Specific",                           
            "settings..."                                                       
  "ISREDIT LINE_BEFORE 1 = NOTELINE '"Message"'"                                
                                                                                
