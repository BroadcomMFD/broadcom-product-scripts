    VPUT (PANELIDP) PROFILE                                                     
  IF (&ZSCREEN = Q)                                                             
    &PANELIDQ = '&ZSCREEN:ENDIE910'                                             
    VPUT (PANELIDQ) PROFILE                                                     
  IF (&ZSCREEN = R)                                                             
    &PANELIDR = '&ZSCREEN:ENDIE910'                                             
    VPUT (PANELIDR) PROFILE                                                     
  IF (&ZSCREEN = S)                                                             
    &PANELIDS = '&ZSCREEN:ENDIE910'                                             
    VPUT (PANELIDS) PROFILE                                                     
  IF (&ZSCREEN = T)                                                             
    &PANELIDT = '&ZSCREEN:ENDIE910'                                             
    VPUT (PANELIDT) PROFILE                                                     
  IF (&ZSCREEN = U)                                                             
    &PANELIDU = '&ZSCREEN:ENDIE910'                                             
    VPUT (PANELIDU) PROFILE                                                     
  IF (&ZSCREEN = V)                                                             
    &PANELIDV = '&ZSCREEN:ENDIE910'                                             
    VPUT (PANELIDV) PROFILE                                                     
  IF (&ZSCREEN = W)                                                             
    &PANELIDW = '&ZSCREEN:ENDIE910'                                             
    VPUT (PANELIDW) PROFILE                                                     
/* End code for ESORT */                                                        
  .HELP = ENDHE910                                                              
  VGET (AMT)                                                                    
  IF (&AMT = '')                                                                
    &AMT = PAGE                                                                 
  /* Adjust the Dialog Prompt text for hidden or expanded */                    
  if (.MSG = FLOW000E)                      /* invalid option?     */           
     if (&VARWKCMD = ZREDISPLAY)            /* and we wanted refsh */           
         &VARWKCMD = &Z                     /* clear it            */           
         .MSG = ISRZ001                     /* and show saved msg  */           
  if (&VNTUIHID = 'Y')                                                          
      &MyPrmpt = 'Dialog Commands: +'                                           
  else                                                                          
      &MyPrmpt = 'Dialog Commands: -'                                           
      &VNTUIHID = 'N'                                                           
)PROC                                                                           
  /* ActionBar Processing - Refresh on screen values if necessary */            
  if (&EABENTR = 'YES')                    /* user wants to jump? */            
     .RESP = END                           /* Press End           */            
  /* ActionBar Processing - Refresh on screen values if necessary */            
  IF (&EABREFSH NE &Z )                    /* anything to refresh */            
     VGET (&EABREFSH) ASIS                 /* get those values    */            
     &savlmsg='&ZEDLMSG.'                  /* save current value  */            
     &zedsmsg='Settings changed'           /* Set a message       */            
     &zedlmsg='Refreshed the following values &EABREFSH' /* opt'nl*/            
     .MSG = ISRZ001                        /* and show message    */            
     &zedlmsg='&SAVLMSG.'                  /* restore old value   */            
     &EABREFSH=&Z                          /* reset the flag      */            
     REFRESH(*)                            /* Refresh screen vars */            
     .CURSOR = VARWKCMD                    /* reposition cursor   */            
     EXIT                                  /* and return to user  */            
  /* Save Var's required for Point-n-Shoot Sort-n-scroll          */            
  &MCURSOR = .CURSOR                   /* What field is cursor in */            
  &MCSRPOS = .CSRPOS                   /* and at what offset      */            
  &MYHEADN = ENDQ0H1                   /* What is it called and   */            
  &MYHEADV = '&ENDQ0H1'                /* What is it's contents   */            
  /* Use the cursor position to perform Sort/Left/Right scrolling */            
  if (&MCURSOR = &MYHEADN) /*          /* is Cursor in heading?   */            
     *REXX(MCURSOR MCSRPOS VARWKCMD ZVERB ZUSER MYHEADN MYHEADV,(ENABPSRT))     
     REFRESH(*)                        /* Refresh updated vars... */            
     .CURSOR = VARWKCMD                /* reposition cursor       */            
  /* .MSG = LONG030I */                /* msg to stop auto-enter  */            
     .RESP   = ENTER                   /* or press enter if suprtd*/            
  /* End of Point-n-shoot section                                 */            
  &VARWKSEL = &EEVETSEL                                                         
  &EEVSELCH = &EEVETSEL                                                         
  VPUT (AMT) PROFILE                                                            
  &VARWKSEL = &EEVETSEL                                                         
  &EEVSELCH = &EEVETSEL                                                         
  if (.CURSOR = MYPRMPT) /* If cursor in Dialog Promt (help area) */            
     if (&VNTUIHID = 'Y')                                                       
         &MyPrmpt  = 'Dialog Commands: -'                                       
         &VNTUIHID = 'N'                                                        
     else                                                                       
         &MyPrmpt  = 'Dialog Commands: +'                                       
         &VNTUIHID = 'Y'                                                        
     VPUT (VNTUIHID) PROFILE                                                    
     &zedsmsg='Help Text Hide:&VNTUIHID'   /* Set a message       */            
     &zedlmsg='Toggled the Endevor Help Text display'                           
     &VARWKCMD = 'ZREDISPLAY'              /* jump to defaults    */            
     .RESP = ENTER                         /* Press Enter         */            
  VPUT (AMT) PROFILE                                                            
)FIELD                                                                          
  FIELD(VARWKCMD) SCROLL(OFF) LEN(255)                                          
  FIELD(EEVETSEL) SCROLL(OFF) /* no scroll in sel field so PF10/11 work */      
  FIELD(EEVETKEL) SCROLL(ON)  LEN(255)                                          
  FIELD(EEVETDMS) SCROLL(ON)  LEN(133)                                          
  FIELD(EEVETKTY) SCROLL(ON)  LEN(008)                                          
  FIELD(EEVETNS ) SCROLL(ON)  LEN(001)                                          
  FIELD(EEVETKEN) SCROLL(ON)  LEN(008)                                          
  FIELD(EEVETKSI) SCROLL(ON)  LEN(008)                                          
  FIELD(EEVETKSN) SCROLL(ON)  LEN(008)                                          
  FIELD(EEVETDSL) SCROLL(ON)  LEN(008)                                          
  FIELD(EEVETKSY) SCROLL(ON)  LEN(008)                                          
  FIELD(EEVETKSB) SCROLL(ON)  LEN(008)                                          
  FIELD(EEVETDVL) SCROLL(ON)  LEN(008)                                          
  FIELD(EEVETPGR) SCROLL(ON)  LEN(008)                                          
  FIELD(EEVETUID) SCROLL(ON)  LEN(008)                                          
  FIELD(EEVETCCI) SCROLL(ON)  LEN(012)                                          
  FIELD(EEVETPRC) SCROLL(ON)  LEN(008)                                          
  FIELD(EEVETNRC) SCROLL(ON)  LEN(008)                                          
  FIELD(EEVETSO ) SCROLL(ON)  LEN(008)                                          
  FIELD(ESRFOUND) SCROLL(ON)  LEN(255)                                          
  FIELD(ESRTGTEL) SCROLL(ON)  LEN(008)                                          
  FIELD(USERDATA) SCROLL(ON)  LEN(080)                                          
  FIELD(EEVCOMM ) SCROLL(ON)  LEN(040)                                          
  FIELD(EEVBCOM ) SCROLL(ON)  LEN(040)                                          
  FIELD(EEVEUSRD) SCROLL(ON)  LEN(080)                                          
)PNTS                                                                           
  FIELD(ZPS00001) VAR(VARWKCMD) VAL('RIGHT')                                    
)END                                                                            
