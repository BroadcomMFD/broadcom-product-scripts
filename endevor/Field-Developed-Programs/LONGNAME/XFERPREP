 PARM('SELECT CMD(PKGELES)')                                                    
PDC DESC('XFerEles...')    MNEM(2) UNAVAIL(ENABPRI) ACTION RUN(ISRROUTE)        
 PARM('SELECT CMD(XFERELES)')                                                   
