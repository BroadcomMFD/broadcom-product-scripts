<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane fx:id="root" minWidth="280" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.broadcom.msd.om.sample.downloader.controllers.Endpoint">
  <GridPane hgap="5.0" vgap="5.0">
    <Label alignment="CENTER_RIGHT" text="Enter the Web Viewer URL." GridPane.columnSpan="2147483647" />

    <Label alignment="CENTER_RIGHT" text="URL:" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
    <TextField fx:id="urlControl" onKeyTyped="#refreshButtons" prefWidth="280.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />

    <Button fx:id="okControl" defaultButton="true" onAction="#submit" text="_OK" GridPane.columnSpan="2147483647" GridPane.rowIndex="2" />

    <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" prefWidth="80.0" />
      <ColumnConstraints hgrow="SOMETIMES" />
    </columnConstraints>

    <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>

    <rowConstraints>
      <RowConstraints vgrow="SOMETIMES" />
      <RowConstraints vgrow="SOMETIMES" />
      <RowConstraints vgrow="SOMETIMES" />
    </rowConstraints>
  </GridPane>
</StackPane>
